digraph DFA {
  rankdir=LR;
  node [shape = circle];

  // 定义状态，带双圆的为接受状态
  start [shape=none, label=""];
  q0 [label="q0", shape=doublecircle];
  q1 [label="q1", shape=doublecircle];
  q2 [label="q2", shape=doublecircle];
  q3 [label="q3", shape=doublecircle];

  // 初始状态指向q0
  start -> q0 [label="start"];

  // 转移函数
  q0 -> q1 [label="0"];
  q0 -> q2 [label="1"];

  q1 -> q1 [label="0"];
  q1 -> q3 [label="1"];

  q2 -> q1 [label="0"];
  q3 -> q1 [label="0"];


}